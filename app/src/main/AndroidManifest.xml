    <application
        android:allowBackup="true"
        android:icon="@mipmap/mywallet_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/WalletTheam">
        <activity android:name=".MainActivity" />
        <activity android:name=".Model.IncomeMasterActivity" />
        <activity android:name=".Model.ExpenseMasterActivity" />
